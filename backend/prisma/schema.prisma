generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Blog {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  title       String   @db.VarChar(100)
  url         String   @db.VarChar(100)
  description String   @db.VarChar(255)
}
